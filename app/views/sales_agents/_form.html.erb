<%= form_for sales_agent do |f| %>
  <%= render 'common/errors', resource: sales_agent %>
  <form>
    <div class="form-group">
      <%= f.label :accounting_system_code, "Код учётной системы" %><br />
      <%= f.text_field :accounting_system_code, autofocus: true, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :IMEI, "IMEI" %><br />
      <%= f.text_field :IMEI, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :name, "ФИО" %><br />
      <%= f.text_field :name, class: "form-control" %>
    </div>

    <div class="actions">
      <%= f.submit "Создать", class: 'btn btn-primary'%>
    </div>
  </form>
<% end %>
